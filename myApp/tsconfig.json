{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/(tabs)/interest-points.jsx", "app/api/shuttleScheduleData.js", "app/server/shuttleApi.js", "app/server/server.js", "app/__tests__/shuttleApi.test.js", "app/api/shuttleLiveData.js", "app/api/shuttleSchedule.js"  ],
    "exclude": [
      "node_modules"
    ]
}
